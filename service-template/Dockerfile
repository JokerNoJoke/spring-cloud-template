FROM openjdk:11
LABEL JOB_NAME=$JOB_NAME BUILD_ID=$BUILD_ID
ENV MAVEN_PROJECT_NAME=$MAVEN_PROJECT_NAME
ENV MAVEN_PROJECT_VERSION=$MAVEN_PROJECT_VERSION
WORKDIR /data/app
COPY target/$MAVEN_PROJECT_NAME-$MAVEN_PROJECT_VERSION .
CMD ["java", "-jar", "$MAVEN_PROJECT_NAME-$MAVEN_PROJECT_VERSION"]
EXPOSE 8080